    <!-- http://purl.org/dc/terms/hasVersion -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/hasVersion">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameTable"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <skos:definition xml:lang="en">A related resource that is a version, edition, or adaptation of the described resource.</skos:definition>
