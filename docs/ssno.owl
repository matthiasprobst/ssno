        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#Dataset"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardName"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">If ssno:standardName is associated with an HDF5 Dataset then the corresponding standard name table must be assigned to the root group in order to elaborate the properties of the ssno:StandardName.</rdfs:comment>
        <skos:definition xml:lang="en">Name for a standard name, e.g. &quot;x_velocity&quot;. The construction of a standard name string is defined by the standard name table. E.g. no spaces are allowed and specific pre- and suffixes are allowed.</skos:definition>
        <skos:example xml:lang="en">x_velocity</skos:example>
        <skos:prefLabel xml:lang="en">standard name</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#standardNameCharacter -->


    <owl:DatatypeProperty rdf:about="https://matthiasprobst.github.io/ssno#standardNameCharacter">
