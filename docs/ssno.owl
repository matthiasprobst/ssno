        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Transformation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Points to a Qualification or Transformation defined by a Standard Name Table</skos:definition>
        <skos:prefLabel xml:lang="en">has modifier</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#hasStandardName -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#hasStandardName">
        <owl:inverseOf rdf:resource="https://matthiasprobst.github.io/ssno#isStandardNameOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#Dataset"/>
                    <rdf:Description rdf:about="http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable"/>
                    <rdf:Description rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#Property"/>
                    <rdf:Description rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#QuantityValue"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <skos:definition xml:lang="en">Points to a ssno:StandardName associated with the Concept. The Concept should is expected to be describing a numerical quantity because a ssno:StandardName defines and describe a numerical quantity by providing a description and expected canonical unit.</skos:definition>
        <skos:prefLabel xml:lang="en">has standard name</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#hasValidValues -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#hasValidValues">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#DomainConceptSet"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#TextVariable"/>
        <skos:definition xml:lang="en">A relationship defining a set of values used by the qualification.</skos:definition>
        <skos:prefLabel xml:lang="en">has valid values</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#isStandardNameOf -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#isStandardNameOf">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#Dataset"/>
                    <rdf:Description rdf:about="http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable"/>
                    <rdf:Description rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#Property"/>
                    <rdf:Description rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#QuantityValue"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Points to a Concept, that describes a numerical variable, which uses the standard name.</skos:definition>
        <skos:prefLabel xml:lang="en">is standard name of</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#positioned -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#positioned">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#Qualification"/>
