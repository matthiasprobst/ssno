    <!-- http://purl.allotrope.org/ontologies/hdf5/1.8#member -->


    <owl:ObjectProperty rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#member">
        <rdfs:domain rdf:resource="http://purl.allotrope.org/ontologies/hdf5/1.8#Group"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#Dataset"/>
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#Group"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Relation between collection and the members of it.</skos:definition>
        <skos:prefLabel xml:lang="en">member</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://purl.allotrope.org/ontologies/hdf5/1.8#rootGroup -->


    <owl:ObjectProperty rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#rootGroup">
        <rdfs:domain rdf:resource="http://purl.allotrope.org/ontologies/hdf5/1.8#File"/>
        <rdfs:range rdf:resource="http://purl.allotrope.org/ontologies/hdf5/1.8#RootGroup"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.allotrope.org/ontologies/hdf5/1.8#"/>
        <skos:definition xml:lang="en">Relation between the HDF file and its root group</skos:definition>
        <skos:prefLabel xml:lang="en">has root group</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#agent -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#agent">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Attribution"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <skos:definition xml:lang="en">The prov:agent property references an prov:Agent which influenced a resource. This property applies to an prov:AgentInfluence, which is given by a subproperty of prov:qualifiedInfluence from the influenced prov:Entity, prov:Activity or prov:Agent.</skos:definition>
        <skos:prefLabel xml:lang="en">agent</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#hadRole -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#hadRole">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/prov#wasRoleIn"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Attribution"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <skos:prefLabel xml:lang="en">had role</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#qualifiedAttribution -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#qualifiedAttribution">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Attribution"/>
        <skos:definition xml:lang="en">Attribution is the ascribing of an entity to an agent. When an entity e is attributed to agent ag, entity e was generated by some unspecified activity that in turn was associated to agent ag. Thus, this relation is useful when the activity is not known, or irrelevant</skos:definition>
        <skos:prefLabel xml:lang="en">qualified attribution</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasRoleIn -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasRoleIn">
        <skos:definition xml:lang="en">prov:wasRoleIn references the association (e.g. between an agent and an activity) in which a role shall be defined. Inverse property of prov:hadRole.</skos:definition>
        <skos:prefLabel xml:lang="en">was role in</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/mbox -->


    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/mbox">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <skos:definition xml:lang="en">A personal mailbox, ie. an Internet mailbox associated with exactly one owner.</skos:definition>
        <skos:prefLabel xml:lang="en">mbox</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#after -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#after">
        <rdfs:subPropertyOf rdf:resource="https://matthiasprobst.github.io/ssno#positioned"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#Qualification"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardName"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Refers to another qualification to indicate the order. The qualification it points to is ordered before this one. It can also point to a standard name, which means that this qualification is used right after it.</skos:definition>
        <skos:prefLabel xml:lang="en">after</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#alias -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#alias">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <skos:definition xml:lang="en">Indicates that a standard name individual is an alias of another.</skos:definition>
        <skos:prefLabel xml:lang="en">alias</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#associatedWith -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#associatedWith">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#Character"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardName"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Associates a character to a thing, which is intended to be a AnyStandardName or a Qualification.</skos:definition>
        <skos:prefLabel xml:lang="en">associated with</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#before -->


