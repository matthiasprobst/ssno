    <!-- https://matthiasprobst.github.io/ssno#Qualification -->


    <owl:Class rdf:about="https://matthiasprobst.github.io/ssno#Qualification">
        <rdfs:subClassOf rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameModification"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://matthiasprobst.github.io/ssno#description"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange>
                    <rdfs:Datatype>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                            <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
                        </owl:unionOf>
                    </rdfs:Datatype>
                </owl:onDataRange>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://matthiasprobst.github.io/ssno#Transformation"/>
        <skos:definition xml:lang="en">A qualification derives an existing standard name by a rule. This may alter the units.</skos:definition>
        <skos:example xml:lang="en">See here https://cfconventions.org/Data/cf-standard-names/docs/guidelines.html</skos:example>
        <skos:prefLabel xml:lang="en">Qualification</skos:prefLabel>
    </owl:Class>
    


