            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardName"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardNameModification"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#Literal"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
        <skos:definition xml:lang="en">Description of a Standard Name, a qualification or a transformation.</skos:definition>
        <skos:prefLabel xml:lang="en">description</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#hasPreposition -->


    <owl:DatatypeProperty rdf:about="https://matthiasprobst.github.io/ssno#hasPreposition">
