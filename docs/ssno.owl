        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#Qualification"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardName"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Refers to another qualification to indicate the order. The qualification it points to is ordered before this one. It can also point to a standard name, which means that this qualification is used right after it.</skos:definition>
        <skos:prefLabel xml:lang="en">after</skos:prefLabel>
    </owl:ObjectProperty>
    


