        <rdfs:range rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <skos:definition xml:lang="en">Relates a standard name table to a dataset, which is published and available for access or download in one or more representations.</skos:definition>
        <skos:prefLabel xml:lang="en">dataset</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#hasCharacter -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#hasCharacter">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#Transformation"/>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#Character"/>
        <skos:definition xml:lang="en">Links a transformation to a character used in the transformation string to represent a standard name or qualification</skos:definition>
