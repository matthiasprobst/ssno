    <!-- http://xmlns.com/foaf/0.1/mbox -->


    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/mbox">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <skos:definition xml:lang="en">A personal mailbox, ie. an Internet mailbox associated with exactly one owner.</skos:definition>
        <skos:prefLabel xml:lang="en">mbox</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#after -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#after">
        <rdfs:subPropertyOf rdf:resource="https://matthiasprobst.github.io/ssno#positioned"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#Qualification"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardName"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Refers to another qualification to indicate the order. The qualification it points to is ordered before this one. It can also point to a standard name, which means that this qualification is used right after it.</skos:definition>
        <skos:prefLabel xml:lang="en">after</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#associatedWith -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#associatedWith">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#CharacterRepresentation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardName"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Associates a character to a thing, which is intended to be a AnyStandardName or a Qualification.</skos:definition>
        <skos:prefLabel xml:lang="en">associated with</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#before -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#before">
        <rdfs:subPropertyOf rdf:resource="https://matthiasprobst.github.io/ssno#positioned"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#Qualification"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#StandardName"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Refers to another qualification to indicate the order. The qualification it points to is ordered after this one. It can also point to a standard name, which means that this qualification is used right before it.</skos:definition>
        <skos:prefLabel xml:lang="en">before</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#canonicalUnits -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#canonicalUnits">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <rdfs:range rdf:resource="http://qudt.org/schema/qudt/Unit"/>
        <rdfs:seeAlso rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#hasUnit"/>
        <skos:closeMatch rdf:resource="http://qudt.org/schema/qudt/unit"/>
        <skos:definition xml:lang="en">The canonical unit of the standard name</skos:definition>
        <skos:prefLabel xml:lang="en">canonical units</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#contact -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#contact">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameTable"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <skos:definition xml:lang="en">Contact agent (person or organization) for the standard name table.</skos:definition>
        <skos:prefLabel xml:lang="en">contact</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#hasCharacter -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#hasCharacter">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#Transformation"/>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#CharacterRepresentation"/>
        <skos:definition xml:lang="en">Links a transformation to a character used in the transformation string to represent a standard name or qualification</skos:definition>
        <skos:prefLabel xml:lang="en">has character</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#hasModifier -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#hasModifier">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameTable"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Transformation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Points to a Qualification or Transformation defined by a Standard Name Table</skos:definition>
        <skos:prefLabel xml:lang="en">has modifier</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#hasStandardName -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#hasStandardName">
        <owl:inverseOf rdf:resource="https://matthiasprobst.github.io/ssno#isStandardNameOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://emmo.info/emmo#EMMO_1eed0732_e3f1_4b2c_a9c4_b4e75eeb5895"/>
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#Dataset"/>
                    <rdf:Description rdf:about="http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable"/>
                    <rdf:Description rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#QuantityValue"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <skos:definition xml:lang="en">Points to a ssno:StandardName associated with the Concept. The Concept should is expected to be describing a numerical quantity because a ssno:StandardName defines and describe a numerical quantity by providing a description and expected canonical unit.</skos:definition>
        <skos:prefLabel xml:lang="en">has standard name</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#isDefinedBy -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#isDefinedBy">
        <owl:inverseOf rdf:resource="https://matthiasprobst.github.io/ssno#hasModifier"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Qualification"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/ssno#Transformation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameTable"/>
        <skos:definition xml:lang="en">Associates a Qualification or Transformation to a Standard Name Table</skos:definition>
        <skos:prefLabel xml:lang="en">is defined by</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#isStandardNameOf -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#isStandardNameOf">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://emmo.info/emmo#EMMO_1eed0732_e3f1_4b2c_a9c4_b4e75eeb5895"/>
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#Dataset"/>
                    <rdf:Description rdf:about="http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable"/>
                    <rdf:Description rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#QuantityValue"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Points to a Concept, that describes a numerical variable, which uses the standard name.</skos:definition>
        <skos:prefLabel xml:lang="en">is standard name of</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#positioned -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#positioned">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <skos:definition xml:lang="en">Relates two qualifiactions or a qualificaiton and a standard name to each other.</skos:definition>
        <skos:prefLabel xml:lang="en">positioned</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#standardNameTable -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#standardNameTable">
        <owl:inverseOf rdf:resource="https://matthiasprobst.github.io/ssno#standardNames"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameTable"/>
        <skos:definition xml:lang="en">Points to the ssno:StandardNameTable defining the ssno:StandardName.</skos:definition>
        <skos:prefLabel xml:lang="en">standard name table</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#standardNameTableUsedBy -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#standardNameTableUsedBy">
        <owl:inverseOf rdf:resource="https://matthiasprobst.github.io/ssno#usesStandardNameTable"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameTable"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#rootGroup"/>
                    <rdf:Description rdf:about="http://www.w3.org/ns/dcat#Dataset"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <skos:definition xml:lang="en">Points to a concept, which uses a standard name table.</skos:definition>
        <skos:prefLabel xml:lang="en">standard name table used by</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#standardNames -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#standardNames">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameTable"/>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <skos:definition xml:lang="en">Points to one or more standard names defined in a standard name table.</skos:definition>
        <skos:prefLabel xml:lang="en">standard names</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#usesStandardNameTable -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/ssno#usesStandardNameTable">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.allotrope.org/ontologies/hdf5/1.8#rootGroup"/>
                    <rdf:Description rdf:about="http://www.w3.org/ns/dcat#Dataset"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://matthiasprobst.github.io/ssno#StandardNameTable"/>
        <skos:definition xml:lang="en">Relates a thing, e.g. a netCDF4 file, an HDF5 file or another resource to a standard name table which is associated to the standard names used in the file.</skos:definition>
        <skos:prefLabel xml:lang="en">uses standard name table</skos:prefLabel>
    </owl:ObjectProperty>
    


